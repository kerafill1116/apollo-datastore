<?xml version="1.0" encoding="utf-8"?>
<queue-entries>
<!--Set the number of max concurrent requests to 10-->
    <queue>
        <name>sendMailQueue</name>
        <rate>5/s</rate>
        <bucket-size>5</bucket-size>
        <max-concurrent-requests>10</max-concurrent-requests>
    </queue>

    <queue>
        <name>utilsQueue</name>
        <rate>5/s</rate>
        <bucket-size>5</bucket-size>
        <max-concurrent-requests>10</max-concurrent-requests>
    </queue>

</queue-entries>